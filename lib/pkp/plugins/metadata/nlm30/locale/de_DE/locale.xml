	<message key="plugins.metadata.nlm30.displayName">NLM 3.0 Metadaten</message>
	<message key="plugins.metadata.nlm30.description">Stellt Schemata und Anwendungsschnittstellen für NLM 3.0 bereit.</message>

	<!-- citation meta-data adapter -->
	<message key="plugins.metadata.nlm30.citationAdapter.displayName">NLM 3.0 Zitationsschnittstelle</message>
	<message key="plugins.metadata.nlm30.citationAdapter.description">Extrahiert Metadaten aus Zitationen oder fügt Metadaten den Zitationen hinzu.</message>

	<!-- citation parsers (citation extraction services) -->
	<message key="plugins.metadata.nlm30.citationParsers.displayName">Zitationsextraktion"</message>
	<message key="plugins.metadata.nlm30.citationParsers.description">Konvertiere eine Literaturliste im Klartext in NLM 3.0-Zitationsauszeichnung."</message>

	<!-- citation lookup (citation database/improvement/lookup services) -->
	<message key="plugins.metadata.nlm30.citationLookup.displayName">Zitationsdatenbankdienste"</message>
	<message key="plugins.metadata.nlm30.citationLookup.description">Verbesserung der NLM 3.0-Zitationsauszeichnung, z.B. durch das Nachschlagen von Zitationen in externen Datenbanken."</message>

