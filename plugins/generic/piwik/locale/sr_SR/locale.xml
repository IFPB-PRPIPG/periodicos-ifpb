<locale name="sr_SR" full_name="Srpski">

	<!-- Piwik -->
	<message key="plugins.generic.piwik.displayName">Piwik dodatak</message>
	<message key="plugins.generic.piwik.description"><![CDATA[INtegriše OJS i Piwik sistem za analizu saobraćaja na web sajtu. Zahtev je da je Piwik već instaliran. Pogledajte <a href="http://piwik.org/" title="piwik site">Piwik sajt</a> za više informacija.]]></message>

	<!-- Piwik Settings Management -->
	<message key="plugins.generic.piwik.manager.settings">Podešavanja</message>
	<message key="plugins.generic.piwik.manager.piwikSettings">Podešavanja Piwik-a</message>
	<message key="plugins.generic.piwik.manager.settings.description"><![CDATA[<p>Sa ovim dodatkom Piwik može da se koristi za analizu saobraćaja na stranicama časopisa. Imajte na umu da ovaj dodatak zahteva da je Piwik već instaliran. Molimo pogledajte <a href="http://piwik.org/" title="Piwik site">Piwik sajt</a> za više informacija</p>.]]></message>
	<message key="plugins.generic.piwik.manager.settings.piwikUrl">Piwik Url</message>
	<message key="plugins.generic.piwik.manager.settings.piwikUrlInstructions">URL do Piwik instalacije (i.e. http://myserver.net/piwik).</message>
	<message key="plugins.generic.piwik.manager.settings.piwikUrlRequired">Unesite ispravan URL sa http:// na početku</message>
	<message key="plugins.generic.piwik.manager.settings.piwikSiteId">ID profila sajta</message>
	<message key="plugins.generic.piwik.manager.settings.piwikSiteIdInstructions"><![CDATA[U Piwik-u kliknite na Settings (podešavanja) -> Websites. ID profila sajta je odmah pored imena profila sajta.]]></message>
	<message key="plugins.generic.piwik.manager.settings.piwikSiteIdRequired">Unesite ID prifila sajta</message>

</locale>
